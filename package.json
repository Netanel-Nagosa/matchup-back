{
  "name": "matchup",
  "scripts": {
    "install-all": "cd server && npm install && cd ../client && npm install",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm start"
  },
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
